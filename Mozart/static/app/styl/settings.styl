.menu-list 
	background white
	border-color mozart-light-blue
	border-style solid
	border-width 0 0 3px 0
	display block
	margin-top 15px
	
	li
		display inline-block
		
		a
			color mozart-light-blue
			display block
			padding 5px
			
			&:before
				font-size 30px
				
			&:hover, &:active
				color mozart-medium-blue
	
#settings-content
	background white
	margin-top 0